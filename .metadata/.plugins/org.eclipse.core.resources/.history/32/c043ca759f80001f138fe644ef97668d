package exercice3;

public class Satellite03 {
	public static void main(String[] args) {
		private int masse;
		private int vitesse;	
		public String nom;
		public Satellite03(double ma,double vi,String nm){
			this.masse=ma.intValue();
			this.vitesse=vi.intValue();
			this.nom=nm;
		}
		public void impulsion(double force,double duree) {
			double res=(force*duree)/masse;
			System.out.println("l'impulsion du sattzlite est : "+res);
		}
		
}
	}

